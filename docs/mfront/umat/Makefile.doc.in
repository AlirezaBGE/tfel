umat:
	TEXMFHOME=@abs_top_srcdir@/docs/tex/texmf        latex  umat.tex
	TEXMFHOME=@abs_top_srcdir@/docs/tex/texmf        latex  umat.tex
	BSTINPUTS=@abs_top_srcdir@/docs/tex/texmf/bibtex bibtex umat
	-makeindex -s @srcdir@/style.ist uenv.idx
	-makeindex -s @srcdir@/style.ist uheaders.idx
	-makeindex -s @srcdir@/style.ist utfel.idx
	-makeindex -s @srcdir@/style.ist umkeys.idx
	TEXMFHOME=@abs_top_srcdir@/docs/tex/texmf        latex  umat.tex
	TEXMFHOME=@abs_top_srcdir@/docs/tex/texmf        latex  umat.tex
	TEXMFHOME=@abs_top_srcdir@/docs/tex/texmf        dvips -R0 umat.dvi -o
	ps2pdf umat.ps

clean:
	-$(RM) *.aux *.log
	-$(RM) *.out *.toc
	-$(RM) *.dvi *.ps
	-$(RM) *.pdf
