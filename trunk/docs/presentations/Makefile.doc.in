doc1=mfront-presentation
doc2=mfront-behaviours

all:
	TEXMFHOME=@abs_top_srcdir@/docs/tex/texmf        latex  $(doc1).tex
	TEXMFHOME=@abs_top_srcdir@/docs/tex/texmf        latex  $(doc1).tex
	TEXMFHOME=@abs_top_srcdir@/docs/tex/texmf        latex  $(doc1).tex
	TEXMFHOME=@abs_top_srcdir@/docs/tex/texmf        dvips -R0 $(doc1).dvi -o
	ps2pdf $(doc1).ps
	TEXMFHOME=@abs_top_srcdir@/docs/tex/texmf        latex  $(doc2).tex
	TEXMFHOME=@abs_top_srcdir@/docs/tex/texmf        latex  $(doc2).tex
	TEXMFHOME=@abs_top_srcdir@/docs/tex/texmf        latex  $(doc2).tex
	TEXMFHOME=@abs_top_srcdir@/docs/tex/texmf        dvips -R0 $(doc2).dvi -o
	ps2pdf $(doc2).ps

clean:
	-$(RM) $(doc1).aux $(doc1).log
	-$(RM) $(doc1).out $(doc1).toc
	-$(RM) $(doc1).dvi $(doc1).ps
	-$(RM) $(doc1).pdf
	-$(RM) $(doc2).aux $(doc2).log
	-$(RM) $(doc2).out $(doc2).toc
	-$(RM) $(doc2).dvi $(doc2).ps
	-$(RM) $(doc2).pdf
